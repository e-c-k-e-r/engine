	"metadata": {
		"graph": {
			"baking": {
				"enabled": true,
				"resolution": 2048
			},
			"renderer": {
				"filter": "linear"
			},
			"tags": {
				"/^worldspawn/": {
					"physics": { "type": "mesh", "static": true },
					"grid": { "size": [3,1,3], "epsilon": 0.0001, "cleanup": true, "print": true },
